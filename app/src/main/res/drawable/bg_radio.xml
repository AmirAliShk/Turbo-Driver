<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true">
        <shape>
            <corners
                android:radius="15dp"/>

            <solid
                android:color="#cb9a43" />


        </shape>
    </item>

    <item android:state_checked="false">
        <shape>

            <corners
                android:radius="15dp"/>

            <solid
                android:color="#efefef"/>



        </shape>
    </item>
</selector>